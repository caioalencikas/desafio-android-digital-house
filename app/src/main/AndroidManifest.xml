<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.caioal.desafio_android_digital_house">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreenAcitivity"
                android:theme="@style/SplashTheme">
        </activity>
        <activity android:name=".PlateActivity" />
        <activity android:name=".ListPlatesActivity" />
        <activity android:name=".account.RegisterActivity" />
        <activity android:name=".ListRestaurantsActivity" />
        <activity android:name=".account.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>